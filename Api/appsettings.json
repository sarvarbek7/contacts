{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Host=localhost; Port=5432; User Id=postgres; Password=postgres; Database=contacts; Include Error Detail=true;"
  },
  "HttpConfigurations": [
    {
      "Name": "hrm",
      "BaseUrl": "https://api-exodim.railway.uz",
      "Endpoints": [
        {
          "Name": "login",
          "Path": "/api/auth/collaborator/login",
          "Method": "Post",
          "Body": "{{\"email\": \"{0}\", \"password\": \"{1}\"}}"
        },
        {
          "Name": "check-worker",
          "Path": "/api/collaborator/cadry/check",
          "Method": "Get",
          "QueryParams": ["pinfl"]
        },
        {
          "Name": "structure",
          "Path": "/api/collaborator/cadry/structure",
          "Method": "Get"
        }
      ]
    },
    {
      "Name": "hrm_pro",
      "BaseUrl": "https://docflow.dasuty.com",
      "Endpoints": [
        {
          "Name": "login",
          "Path": "/api/auth/login",
          "Method": "Post",
          "Body": "{{\"phone\": \"{0}\", \"password\": \"{1}\"}}"
        },
        {
          "Name": "structure",
          "Path": "/api/v1/hr/report/structure",
          "Method": "Get"
        },
        {
          "Name": "departments",
          "Path": "/api/v1/hr/report/departments",
          "Method": "Get",
          "QueryParams": ["page", "per_page", "organization_id"]
        },
        {
          "Name": "positions",
          "Path": "/api/v1/hr/report/department-positions",
          "Method": "Get",
          "QueryParams": ["page", "per_page", "organization_id", "department_id"]
        },
        {
          "Name": "workers",
          "Path": "/api/v1/hr/report/worker-positions",
          "Method": "Get",
          "QueryParams": ["page", "per_page", "organization_id", "department_id", "department_position_id"]
        }
      ]
    }
  ],
  "Credentials": {
    "HRM": {
      "Email": "write email",
      "Password": "write password"
    },
    "HRM_PRO": {
      "Phone": "write phone",
      "Password": "write password"
    }
  }
}
